
<h1>Validator Login User</h1>
 
<%= form_with model: @user, local: true do |form| %>
  
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>
 
  <p>
    <%= form.label :image %><br>
    <%= form.text_area :image, rows: 20 %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
 
<% end %>
 
<%= link_to 'Back', users_path %>